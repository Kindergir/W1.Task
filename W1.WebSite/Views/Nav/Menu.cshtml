@model IDictionary<string, string>

    @Html.ActionLink("Home", "List", "Product")

    @foreach (var link in Model)
    {
    @Html.RouteLink(link.Value, new
    {
    controller = "Product",
    action = "List",
    category = link.Value,
    page = 1
    },
  new {
    @class = link.Value == ViewBag.SelectedCategory ? "selected" : null
  })
    }